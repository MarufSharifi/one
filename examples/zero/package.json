{
  "name": "example-zero",
  "version": "1.1.237",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vxs dev",
    "dev:clean": "vxs dev --clean",
    "clean": "vxs clean",
    "prod": "vxs build && vxs serve",
    "build:prod": "vxs build",
    "serve": "vxs serve",
    "orm:generate": "drizzle-kit generate --config ./db/drizzle.config.ts",
    "upgrade:tamagui": "yarn up '*tamagui*' '@tamagui/*'",
    "orm:push": "drizzle-kit push --config ./db/drizzle.config.ts",
    "orm:migrate": "dotenvx run -f .env -- tsx db/run-migrations.ts",
    "orm:seed": "dotenvx run -f .env -- tsx db/seed.ts"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.12.1",
    "@tamagui/config": "^1.112.0",
    "@tamagui/image-next": "^1.112.0",
    "@tamagui/lucide-icons": "^1.112.0",
    "@tamagui/vite-plugin": "^1.112.0",
    "@vxrn/color-scheme": "1.1.237",
    "better-sqlite3": "^11.2.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.24.2",
    "drizzle-orm": "^0.33.0",
    "expo": "^51.0.32",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-native": "0.74.1",
    "react-native-reanimated": "npm:react-native-reanimated@3.13.0-rc.1",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "^0.19.12",
    "tamagui": "^1.112.0",
    "vxs": "1.1.237"
  },
  "devDependencies": {
    "@faker-js/faker": "next",
    "tsx": "^4.19.0",
    "vite": "6.0.0-beta.1"
  }
}
